#market_trades_wrapper.has-right-dropdown.has-right-dropdown

  .dropdown-wrapper
    ul.dropdown-menu
      li: a.all.active href='#' = t('.all_html')
      li: a.my href='#' = t('.my_html')
  #market_trades
    .panel.panel-default
      .panel-body.panel-body-head.section-title
        table.table
          thead: tr
            th.col-xl-16.text-center
              span.name = 'TRADE HISTORY' 
      .panel-body.panel-body-head
        table.table
          thead: tr
            th.at.col-xs-7.text-left #{t('.time')}
            th.my.col-xs-3.text-left
            th.price.col-xs-7.text-center #{t('.price')}
            th.volume.col-xs-7.text-center #{t('.volume')}
      .panel-body.panel-body-content
        table.table.table-hover.table-striped.all-trades
          tbody
        table.table.table-hover.my-trades
          tbody
