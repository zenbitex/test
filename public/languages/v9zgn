  var w = 1280;
  var h = 1600;

  var areOverlapping = function (el1, el2) {
    var rect1 = el1.getBoundingClientRect();
    var rect2 = el2.getBoundingClientRect();
    return !(rect1.right < rect2.left ||
      rect1.left > rect2.right ||
      rect1.bottom < rect2.top ||
      rect1.top > rect2.bottom);
  }

  var layout = function () {

    if (isMobile()) {
      
      var padding = 60;
      
    } else {
      
      var padding = 120;
      
    }

    var height;
    if (h > window.innerHeight - padding) {
      height = window.innerHeight - padding;
      var MIN_HEIGHT = 140;
      var minHeight = h < MIN_HEIGHT ? h : MIN_HEIGHT;
      if (height < minHeight) {
        height = minHeight;
      }
    } else {
      height = h;
    }
    var width = w / h * height;
    if (width > 800 && window.innerWidth < 1200) {
      width = 800;
    }
    document.getElementById('player-content').style.visibility = "visible";
    document.getElementById('player-content').style.maxWidth = width + "px";
    document.getElementById('filler').style.maxWidth = width + "px";
    document.getElementById('filler').style.paddingBottom = 125.0 + "%"
    document.getElementById('media-container').style.maxWidth = width + "px";


