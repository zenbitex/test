@media (max-width: 768px) {
    .intro {
       margin-top:0 !important;
    }
    .banner-pair {
        position: absolute;
        height: auto;
        padding-top: 10px;
        font-size: 16px;
        margin-top: -40px;
        padding-bottom: 10px;
    }
    .ticker-pair {
        display: inline-block;
        padding-left: 10px;
        padding-right: 10px;
    }
    #index_menu {
        float: none;

    }
    ul#index_menu {
        text-align: center;
    }
    .navbar-nav .open .dropdown-menu {
            float: none;
            width: 101%;
            left: -1px;
            background-color: rgb(0, 0, 0);
            border: 0;
            -webkit-box-shadow: none;
            box-shadow: none;
            margin: 0;
            padding: 0;
            position: absolute;
            z-index: 10000;
        }

    .navbar-default .navbar-toggle, .navbar-default .navbar-toggle:focus, .navbar-default .navbar-toggle:hover, .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover {
        /* background-color: #2f2f2f; */
        
        border-color: #ddd;
        color: white;
        background-color: #1c294f;
    }
    
    .navbar-default .navbar-collapse, .navbar-default .navbar-form {
       border-color: #e7e7e7;
       color: white;
       background-color: #1c294f;
       text-align: center;
       font-weight: 100;
    }
    .navbar-nav > li > a > span, .navbar-nav > li > a > i {
       text-transform: uppercase;
       color: #ffffff;
       font-size: 14px;
       font-weight: 500;
    }
    .navbar-brand {
        display:inline-block;
        width:150px !important;
    }
    .navbar-brand img {
        width: 110px !important;
    }
    .navbar-default {
        /* background-color: #2f2f2f; */
    }
    .navbar {
       padding:0;
    }
    .navbar-header {
        display: inline-block;
        float: right;
        position: absolute;
        top: 10px;
        right: 30px;
    }
    header.navbar.navbar-default.navbar-static-top {
        /* margin-bottom: 33px; */
        margin-bottom: 20px;
    }

    .index-block1-head {
        padding-top: 40px;
    }
    .index-block1-dsc{
        margin: 5px;
    }
    .index-block2 .col-sm-8 {
        display: block;
        float: none;
        margin: 0 auto;
    }
   .index-block2 img {
       width: 47px;
       margin-right: 5px;
    }
    .index-block2-dsc {
    /* text-align: left; */
       justify-content: center !important;
       text-align: justify !important;
    }
    #wrap3 {
      margin-top:0px !important;
   }
    #wrap3 .banner {
      top:0;
    }
    .intro__wrapper {
      margin-top:0;
   }
   .intro__wrapper img{
      width: 120px;
   }
}



